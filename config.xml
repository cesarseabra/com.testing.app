<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bluetooth.test" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>BT.test</name>
    <description>
       Testing Bluetooth
    </description>
    <author email="cesar.seabra.95@gmail.com" href="http://google.pt">
        Cesar Seabra
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="FullScreen" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#1F2B38" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="FadeSplashScreenDuration" value="1000" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="cordova.plugins.diagnostic.modules" value="LOCATION" />
    <platform name="android">
        <icon density="ldpi" src="www/res/icon/android/36.png" />
        <icon density="mdpi" src="www/res/icon/android/48.png" />
        <icon density="hdpi" src="www/res/icon/android/72.png" />
        <icon density="xhdpi" src="www/res/icon/android/96.png" />
        <icon density="xxhdpi" src="www/res/icon/android/144.png" />
        <icon density="xxxhdpi" src="www/res/icon/android/192.png" />
        <splash density="land-ldpi" src="www/res/screen/android/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="www/res/screen/android/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="www/res/screen/android/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="www/res/screen/android/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="www/res/screen/android/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="www/res/screen/android/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="www/res/screen/android/240_320.png" />
        <splash density="port-mdpi" src="www/res/screen/android/320_480.png" />
        <splash density="port-hdpi" src="www/res/screen/android/480_800.png" />
        <splash density="port-xhdpi" src="www/res/screen/android/720_1280.png" />
        <splash density="port-xxhdpi" src="www/res/screen/android/960_1600.png" />
        <splash density="port-xxxhdpi" src="www/res/screen/android/1280_1920.png" />
    </platform>
    <platform name="ios">
        <icon height="57" platform="ios" src="www/res/icon/ios/57.png" width="57" />
        <icon height="114" platform="ios" src="www/res/icon/ios/114.png" width="114" />
        <icon height="40" platform="ios" src="www/res/icon/ios/40.png" width="40" />
        <icon height="80" platform="ios" src="www/res/icon/ios/80.png" width="80" />
        <icon height="50" platform="ios" src="www/res/icon/ios/50.png" width="50" />
        <icon height="100" platform="ios" src="www/res/icon/ios/100.png" width="100" />
        <icon height="60" platform="ios" src="www/res/icon/ios/60.png" width="60" />
        <icon height="120" platform="ios" src="www/res/icon/ios/120.png" width="120" />
        <icon height="180" platform="ios" src="www/res/icon/ios/180.png" width="180" />
        <icon height="72" platform="ios" src="www/res/icon/ios/72.png" width="72" />
        <icon height="144" platform="ios" src="www/res/icon/ios/144.png" width="144" />
        <icon height="76" platform="ios" src="www/res/icon/ios/76.png" width="76" />
        <icon height="152" platform="ios" src="www/res/icon/ios/152.png" width="152" />
        <icon height="29" platform="ios" src="www/res/icon/ios/29.png" width="29" />
        <icon height="58" platform="ios" src="www/res/icon/ios/58.png" width="58" />
        <icon height="87" platform="ios" src="www/res/icon/ios/87.png" width="87" />
        <splash height="1136" platform="ios" src="www/res/screen/ios/640_1136.png" width="640" />
        <splash height="1334" platform="ios" src="www/res/screen/ios/750_1334.png" width="750" />
        <splash height="2208" platform="ios" src="www/res/screen/ios/Default-736h.png" width="1242" />
        <splash height="1242" platform="ios" src="www/res/screen/ios/Default-Landscape-736h.png" width="2208" />
        <splash height="1536" platform="ios" src="www/res/screen/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="768" platform="ios" src="www/res/screen/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="2048" platform="ios" src="www/res/screen/ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="1024" platform="ios" src="www/res/screen/ios/768_1024.png" width="768" />
        <splash height="960" platform="ios" src="www/res/screen/ios/640_960.png" width="640" />
        <splash height="480" platform="ios" src="www/res/screen/ios/320_480.png" width="320" />
    </platform>
    <platform name="ios">
        <config-file parent="UISupportedExternalAccessoryProtocols" target="*-Info.plist">
            <array>
                <string>com.zebra.rawport</string>
            </array>
        </config-file>
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
            <string>This app needs to access your location to manage sales.</string>
        </edit-config>
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-battery-status" spec="^2.0.2" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="^2.0.2" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-device-orientation" spec="^1.0.7" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <plugin name="cordova-plugin-globalization" spec="^1.0.9" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-bluetoothClassic-serial" spec="^0.9.8" />
    <plugin name="cordova-plugin-navigationbar" spec="https://github.com/cranberrygame/cordova-plugin-navigationbar" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-sumup" spec="git+https://github.com/nuvoPoint/cordova-plugin-sumup.git">
        <variable name="SUMUP_API_KEY" value="1204994d-c495-4e18-9b7e-08d7a7386605" />
    </plugin>
    <plugin name="cordova.plugins.diagnostic" spec="^4.0.8" />
    <engine name="browser" spec="^5.0.3" />
    <engine name="android" spec="^7.0.0" />
</widget>
